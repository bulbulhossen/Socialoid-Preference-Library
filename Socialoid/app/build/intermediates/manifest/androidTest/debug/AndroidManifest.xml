<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.curtrostudios.socialoid.test" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="23" />

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:functionalTest="false"
        android:handleProfiling="false"
        android:label="Tests for com.curtrostudios.socialoid.test"
        android:targetPackage="com.curtrostudios.socialoid.test" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/MaterialTheme" >
        <uses-library android:name="android.test.runner" />

        <activity android:name="com.curtrostudios.socialoid.SocialoidPref" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>